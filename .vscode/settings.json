{
  "cmake.ctestEnabled": true,
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.sourceDirectory": "/Users/alexwaldmann/Desktop/AIO Server/cmake",
  "cmake.generator": "Unix Makefiles",
  "cmake.configureOnOpen": true,
  "C_Cpp.intelliSenseEngine": "disabled",
  "clangd.arguments": ["--compile-commands-dir=build/generated/cmake"],
  "makefile.configurations": [
    {
      "name": "Default",
      "makeDirectory": "${workspaceFolder}/cmake",
      "makefilePath": "${workspaceFolder}/cmake/Makefile"
    }
  ],
  "files.associations": {
    "LICENSE": "plaintext"
  },
  "coverage-gutters.coverageFileNames": [
    "lcov.info",
    "coverage.lcov",
    "coverage/lcov.info"
  ],
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": true,
  "python-envs.pythonProjects": [],
  "python-envs.defaultEnvManager": "ms-python.python:system"
}
